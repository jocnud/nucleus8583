<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<nucleus8583-oim xmlns="http://www.nucleus8583.org/schema/nucleus8583-oim"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://www.nucleus8583.org/schema/nucleus8583-oim 
                                     http://www.nucleus8583.org/schema/nucleus8583-oim.xsd">
    
	<message name="msg1">
		<basic no="0" name="mti" type="text" />
		<basic no="2" name="cardNumber" type="text" />
		<basic no="7" name="transmissionDateTime" type="dateTime" pattern="MMddHHmmss" />
		<basic no="11" name="stan" type="int" />
		<basic no="12" name="transactionTime" type="dateTime" pattern="HHmmss" />
		<basic no="13" name="transactionDate" type="dateTime" pattern="MMdd" />
        
		<record no="29" mode="text">
			<pad padWith="C" mode="text" />
			<basic name="charge" type="signedNumeric" length="8" precision="2" signFirst="false" />
		</record>
        
		<basic no="37" name="rrn" type="text" />
		<basic no="39" name="responseCode" type="text" />
		
        <record no="48" mode="text">
            <list-count name="details" type="int" length="2" />
            
            <list name="details" mode="text">
                <basic name="z" type="text" length="8" />
            </list>
        </record>
	</message>
    
    <message name="msg1_rev">
        <basic no="0" name="mti" type="text" />
        <basic no="2" name="cardNumber" type="text" />
        <basic no="7" name="transmissionDateTime" type="dateTime" pattern="MMddHHmmss" />
        <basic no="11" name="stan" type="int" alignment="right" padWith="0" />
        <basic no="12" name="transactionTime" type="dateTime" pattern="HHmmss" />
        <basic no="13" name="transactionDate" type="dateTime" pattern="MMdd" />
        
        <record no="29" mode="text">
            <skip length="1" />
            <basic name="charge" type="signedNumeric" length="8" precision="2" signFirst="false" />
        </record>
        
        <basic no="37" name="rrn" type="text" />
        <basic no="39" name="responseCode" type="text" />
        
        <record no="48" mode="text">
            <list-count name="details" type="int" length="2" />
            
            <list name="details" mode="text">
                <basic name="z" type="text" length="8" />
            </list>
        </record>
    </message>
</nucleus8583-oim>
